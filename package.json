{
  "name": "living-garden",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "npx netlify dev",
    "dev:aws": "npx dotenvx run --env-file=.env -- npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev:drizzle-generate": "npx dotenvx run --env-file=.env.development -- npx drizzle-kit generate",
    "dev:drizzle-push": "npx dotenvx run --env-file=.env.development -- npx drizzle-kit push",
    "dev:drizzle-studio": "npm run dev:drizzle-generate && npm run dev:drizzle-push && npx dotenvx run --env-file=.env.development -- npx drizzle-kit studio",
    "dev:db-setup": "npm run dev:drizzle-generate && npm run dev:drizzle-push",
    "drizzle-generate": "npx drizzle-kit generate",
    "drizzle-push": "npx drizzle-kit push",
    "drizzle-studio": "npm run drizzle-generate && npm run drizzle-push && npx drizzle-kit studio"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.26.0",
    "@netlify/functions": "^2.8.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-netlify": "^4.2.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.21.4",
    "lucia": "^3.2.0",
    "netlify-cli": "^17.33.3",
    "pg": "^8.11.3",
    "postcss": "^8.4.38",
    "prisma": "^5.17.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.4.1",
    "tsx": "^4.16.3",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.536.0",
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/credential-providers": "^3.535.0",
    "@aws-sdk/lib-storage": "^3.563.0",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@prisma/client": "^5.17.0",
    "@zxing/library": "^0.21.0",
    "apollo-server-svelte-kit": "^3.10.0",
    "colorthief": "^2.4.0",
    "drizzle-graphql": "^0.8.4",
    "drizzle-orm": "^0.30.10",
    "graphql": "^16.9.0",
    "graphql-http": "^1.22.1",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "^2.0.5",
    "openai": "^4.29.0",
    "postgres": "^3.4.4",
    "q-floodfill": "^1.3.1",
    "qrcode": "^1.5.3",
    "random-elements": "^0.3.0",
    "tether-agent": "^3.2.0",
    "uuid": "^9.0.1"
  }
}
