{
  "name": "living-garden",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx netlify dev",
    "dev:https": "USE_HTTPS=true npx netlify dev",
    "dev:aws": "npx dotenvx run --env-file=.env -- npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev:drizzle-generate": "npx dotenvx run --env-file=.env.development -- npx drizzle-kit generate",
    "dev:drizzle-push": "npx dotenvx run --env-file=.env.development -- npx drizzle-kit push",
    "dev:drizzle-studio": "npm run dev:drizzle-generate && npm run dev:drizzle-push && npx dotenvx run --env-file=.env.development -- npx drizzle-kit studio",
    "dev:db-setup": "npm run dev:drizzle-generate && npm run dev:drizzle-push",
    "drizzle-generate": "npx drizzle-kit generate",
    "drizzle-push": "npx drizzle-kit push",
    "drizzle-studio": "npm run drizzle-generate && npm run drizzle-push && npx drizzle-kit studio",
    "cy:open": "cypress open",
    "cy:headless": "cypress run --browser chrome"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.26.0",
    "@eslint/js": "^9.12.0",
    "@netlify/functions": "^2.8.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-netlify": "^4.2.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/howler": "^2.2.12",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/pg": "^8.11.10",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.13.2",
    "cypress-repeat": "^2.3.6",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "lucia": "^3.2.0",
    "netlify-cli": "^17.33.3",
    "pg": "8.13",
    "postcss": "^8.4.38",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.0.3",
    "vite-plugin-mkcert": "^1.17.6"
  },
  "type": "module",
  "dependencies": {
    "@anselan/maprange": "^0.10.0",
    "@aws-sdk/client-rds-data": "^3.536.0",
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/credential-providers": "^3.535.0",
    "@aws-sdk/lib-storage": "^3.563.0",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@zxing/library": "^0.21.0",
    "colorthief": "^2.4.0",
    "drizzle-orm": "^0.33.0",
    "howler": "^2.2.4",
    "luxon": "^3.4.4",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "openai": "^4.29.0",
    "pixi.js": "^8.3.2",
    "postgres": "^3.4.4",
    "q-floodfill": "^1.3.1",
    "qrcode": "^1.5.3",
    "random-elements": "^0.4.1",
    "svelte-transition-classes": "^0.1.1",
    "tether-agent": "^3.2.0",
    "uuid": "^9.0.1",
    "written-number": "^0.11.1"
  }
}
